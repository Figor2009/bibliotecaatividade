// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client-js"
}

model Users {
  username String @unique
  password String
}

model Books {
  title String
  author String @unique
}

datasource db {
  provider = "sqlite"
  url      = "file:./outro_banco.db"
}
